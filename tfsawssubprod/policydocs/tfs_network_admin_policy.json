{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "ec2:*Vpn*",
                "ec2:*Interface*",
                "ec2:*NetworkAcl*",
                "ec2:*Route*",
                "ec2:*Vpc*",
                "ec2:*Address*",
                "ec2:*DhcpOptions*",
                "ec2:*Gateway*",
                "ec2:*SecurityGroup*",
                "ec2:*Subnet*",
                "ec2:*Flowlogs",
                "acm:ListCertificates",
                "acm:ImportCertificate",
                "acm:ExportCertificate",
                "acm:ListTagsForCertificate",
                "acm:DescribeCertificate",
                "acm:GetCertificate",
                "elasticloadbalancing:*"
            ],
            "Resource": "*"
        }
    ]
}
