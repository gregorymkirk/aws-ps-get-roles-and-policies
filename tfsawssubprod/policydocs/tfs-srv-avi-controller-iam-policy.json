{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "iam:GetPolicyVersion",
                "iam:GetPolicy",
                "iam:ListPolicyVersions",
                "iam:ListRoles"
            ],
            "Resource": [
                "arn:aws:iam::*:policy/tfs-srv-avi*",
                "arn:aws:iam::*:role/tfs-srv-avi-controller-role"
            ]
        },
        {
            "Sid": "VisualEditor1",
            "Effect": "Allow",
            "Action": [
                "iam:ListPolicies",
                "iam:GetRole",
                "iam:GetInstanceProfile",
                "iam:ListAttachedRolePolicies",
                "iam:ListRolePolicies",
                "iam:GetRolePolicy"
            ],
            "Resource": [
                "arn:aws:iam::*:role/tfs-srv-avi-controller-role",
                "arn:aws:iam::*:instance-profile/tfs-srv-avi-controller-role ",
                "arn:aws:iam::*:policy/tfs-srv-avi*",
                "arn:aws:iam::*:role/vmimport"
            ]
        }
    ]
}
