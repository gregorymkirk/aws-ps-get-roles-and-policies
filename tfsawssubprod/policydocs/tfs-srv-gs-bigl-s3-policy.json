{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:ListBucket",
                "s3:DeleteObject"
            ],
            "Resource": [
                "arn:aws:s3:::tfsawssubprod-gs-bigl-hub-bucket",
                "arn:aws:s3:::tfsawssubprod-gs-bigl-hub-bucket/*",
                "arn:aws:s3:::tfsawssubprod-gs-bigl-stage-bucket",
                "arn:aws:s3:::tfsawssubprod-gs-bigl-stage-bucket/*",
                "arn:aws:s3:::tfsawssubprod-gs-bigl-working-bucket",
                "arn:aws:s3:::tfsawssubprod-gs-bigl-working-bucket/*"
            ],
            "Effect": "Allow"
        },
        {
            "Action": [
                "s3:HeadBucket"
            ],
            "Resource": "*",
            "Effect": "Allow"
        }
    ]
}
