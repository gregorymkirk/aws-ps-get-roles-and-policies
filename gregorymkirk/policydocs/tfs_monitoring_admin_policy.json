{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Action": [
        "cloudwatch:*"
      ],
      "Resource": "*",
      "Effect": "Allow"
    },
    
{
    "Effect": "Deny",
    "Action": [
        "lambda:GetFunction",
        "athena:GetQueryResults",
        "s3:getObject",
        "s3:GetObjectTorrent",
        "sdb:GetAttributes",
        "dynamodb:BatchGet*",
        "dynamodb:Get*",
        "dynamodb:Query",
        "dynamodb:Scan"
    ],
    "Resource": "*"

}

  ]
}

