{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "VisualEditor0",
      "Effect": "Allow",
      "Action": [
        "ec2:*Volume*",
        "elasticfilesystem:*",
        "ec2:Describe*",
        "glacier:*",
        "s3:*",
        "dlm:*",
        "backup:*",
        "ec2:*Tag*",
        "ec2:*Interface*",
        "redshift:*Snapshot*",
        "rds:*Snapshot*",
        "storagegateway:*",
        "iam:PassRole",
        "tag:*",
        "resource-groups:*",
        "cloudformation:DescribeStacks",
        "cloudformation:ListStackResources",
        "ec2:*Snapshot*"
      ],
      "Resource": "*"
    },    
    {
      "Effect": "Deny",
      "Action": [
          "lambda:GetFunction",
          "athena:GetQueryResults",
          "s3:getObject",
          "s3:GetObjectTorrent",
          "sdb:GetAttributes",
          "dynamodb:BatchGet*",
          "dynamodb:Get*",
          "dynamodb:Query",
          "dynamodb:Scan"
      ],
      "Resource": "*"
    }
  ]
}

