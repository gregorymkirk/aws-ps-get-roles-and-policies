{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VisualEditor0",
            "Effect": "Allow",
            "Action": [
                "acm:ExportCertificate",
                "acm:DeleteCertificate",
                "ec2:*Vpn*",
                "ec2:*Interface*",
                "elasticloadbalancing:*",
                "ec2:*NetworkAcl*",
                "ec2:*Route*",
                "acm:ListTagsForCertificate",
                "acm:DescribeCertificate",
                "acm:GetCertificate",
                "ec2:*Vpc*",
                "acm:ListCertificates",
                "ec2:*Address*",
                "acm:ImportCertificate",
                "ec2:*DhcpOptions*",
                "ec2:*Gateway*",
                "ec2:*SecurityGroup*",
                "ec2:*Subnet*",
                "ec2:AllocateAddress",
                "ec2:AssociateAddress"
                
            ],
            "Resource": "*"
        }
    ]
}
